
<h1>Farms</h1>

<table>
  <thead>
    <tr>
    <th>User ID </th>
      <th>Farm name</th>
      <th>Farm type</th>
      <th>Pet type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @farms.each do |farm| %>
      <tr>
      <td><%= farm.user_id %></td>
        <td><%= farm.farm_name %></td>
        <td><%= farm.farm_type %></td>
        <td><%= farm.pet_type %></td>
  
       
        <td><%= link_to 'Destroy', farm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


